html, body
  margin:0px
  padding:0px
  border:0px
body
  font:14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color:#000000
  text-decoration:none
table, th, td
  border:1px solid #BBB
  border-collapse:collapse
table
  border-bottom:0
  border-left:0
td, th
  text-align: left
  border-top:0
  border-right:0
li, ol
  text-decoration:none
  display:inline
  margin:0
  padding:0
h3, p
  text-align:center
#loadNext
  cursor:pointer
li:not(:first-child) {
  margin-left:10px
  list-style-type:disc
}